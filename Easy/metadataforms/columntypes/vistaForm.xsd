<?xml version="1.0" standalone="yes"?>
<xs:schema id="vistaForm" targetNamespace="http://tempuri.org/vistafrmcolumntypes.xsd" xmlns:mstns="http://tempuri.org/vistafrmcolumntypes.xsd" xmlns="http://tempuri.org/vistafrmcolumntypes.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
  <xs:element name="vistaForm" msdata:IsDataSet="true">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element name="columntypes">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="tablename" type="xs:string" />
              <xs:element name="field" type="xs:string" />
              <xs:element name="iskey" type="xs:string" />
              <xs:element name="sqltype" type="xs:string" />
              <xs:element name="col_len" type="xs:int" minOccurs="0" />
              <xs:element name="col_precision" type="xs:int" minOccurs="0" />
              <xs:element name="col_scale" type="xs:int" minOccurs="0" />
              <xs:element name="systemtype" type="xs:string" minOccurs="0" />
