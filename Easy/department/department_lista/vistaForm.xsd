<?xml version="1.0" encoding="utf-8" ?>
<xs:schema id="vistaForm" targetNamespace="http://tempuri.org/vistaForm.xsd" elementFormDefault="qualified"
	attributeFormDefault="qualified" xmlns="http://tempuri.org/vistaForm.xsd" xmlns:mstns="http://tempuri.org/vistaForm.xsd"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xs:element name="vistaForm" msdata:IsDataSet="true">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="department">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="iddepartment" type="xs:int" />
							<xs:element name="description" type="xs:string" />
							<xs:element name="server" type="xs:string" minOccurs="0" />
							<xs:element name="db" type="xs:string" minOccurs="0" />
							<xs:element name="userdep" type="xs:string" minOccurs="0" />
							<xs:element name="ct" type="xs:dateTime" />
							<xs:element name="cu" type="xs:string" />
							<xs:element name="lt" type="xs:dateTime" />
							<xs:element name="lu" type="xs:string" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
		<xs:unique name="vistaFormKey1" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:department" />
			<xs:field xpath="mstns:iddepartment" />
		</xs:unique>
	</xs:element>
</xs:schema>