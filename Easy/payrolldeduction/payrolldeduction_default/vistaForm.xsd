<?xml version="1.0" standalone="yes"?>
<xs:schema id="vistaForm" targetNamespace="http://tempuri.org/VistaDettaglioDeduzioneCedolino.xsd" xmlns:mstns="http://tempuri.org/VistaDettaglioDeduzioneCedolino.xsd" xmlns="http://tempuri.org/VistaDettaglioDeduzioneCedolino.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
  <xs:element name="vistaForm" msdata:IsDataSet="true" msdata:Locale="en-US">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
