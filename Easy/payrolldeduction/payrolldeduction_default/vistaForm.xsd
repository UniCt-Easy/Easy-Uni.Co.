      <xs:field xpath="mstns:iddeduction" />
    </xs:keyref>
    <xs:keyref name="taxablekindpayrolldeduction" refer="Constraint2">
      <xs:selector xpath=".//mstns:payrolldeduction" />
      <xs:field xpath="mstns:taxablecode" />
    </xs:keyref>
