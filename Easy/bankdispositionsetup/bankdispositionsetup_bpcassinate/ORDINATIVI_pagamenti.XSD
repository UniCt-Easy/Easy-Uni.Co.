          <xs:simpleType>
            <xs:restriction base="xs:decimal">
              <xs:totalDigits value="17"/>
              <xs:fractionDigits value="2"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="conto_evidenza" minOccurs="0">
          <xs:annotation>
            <xs:documentation>Conto evidenza concordato fra la PA e la BT</xs:documentation>
          </xs:annotation>
          <xs:simpleType>
            <xs:restriction base="xs:string">
