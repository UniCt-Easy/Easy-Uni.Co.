            </xs:restriction>
          </xs:simpleType>
        </xs:element>

        <xs:element ref="informazioni_versante" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="mandato">
