          <xs:simpleType>
            <xs:restriction base="xs:integer">
              <xs:totalDigits value="5"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="data_reversale" type="xs:date">
          <xs:annotation>
            <xs:documentation>Data della reversale nel formato "SSAA-MM-GG"</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="importo_reversale">
