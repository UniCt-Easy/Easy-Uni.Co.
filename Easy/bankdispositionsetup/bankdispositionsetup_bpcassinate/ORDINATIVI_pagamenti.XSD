<xs:schema id="Sepa_pagamenti" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
  <xs:element name="flusso_ordinativi">
    <xs:annotation>
      <xs:documentation>Flusso XML per Ordinativi informatici</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="identificativo_flusso">
          <xs:annotation>
            <xs:documentation>Univoco a livello di esercizio</xs:documentation>
          </xs:annotation>
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:minLength value="1"/>
              <xs:maxLength value="30"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="esercizio" type="xs:gYear">
          <xs:annotation>
            <xs:documentation>Anno di esercizio nel formato "SSAA"</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="data_ora_creazione_flusso" type="xs:dateTime" minOccurs="0">
          <xs:annotation>
            <xs:documentation>Data e ora di creazione del flusso secondo il formalisno ISO8601 "SSAA-MM-GGTHH:MM:SS"</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="codice_ABI_BT">
          <xs:annotation>
            <xs:documentation>Codice ABI banca tesoreria</xs:documentation>
          </xs:annotation>
          <xs:simpleType>
            <xs:restriction base="xs:integer">
              <xs:totalDigits value="5"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="codice_ente">
          <xs:annotation>
            <xs:documentation>Partita IVA dell'ente oppure codice MIUR</xs:documentation>
          </xs:annotation>
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:maxLength value="11"/>
              <xs:minLength value="10"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="descrizione_ente">
          <xs:annotation>
            <xs:documentation>Denominazione dell'ente</xs:documentation>
          </xs:annotation>
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:maxLength value="90"/>
              <xs:minLength value="1"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="codice_ente_BT">
          <xs:annotation>
            <xs:documentation>Codice ente presso tesoriere</xs:documentation>
          </xs:annotation>
          <xs:simpleType>
            <xs:restriction base="xs:integer">
              <xs:totalDigits value="3"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="riferimento_ente" minOccurs="0">
          <xs:annotation>
            <xs:documentation>Codice concordato tra PA e BT per particolari esigenze</xs:documentation>
          </xs:annotation>
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:maxLength value="12"/>
              <xs:minLength value="0"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element ref="reversale" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="mandato" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="reversale">
    <xs:annotation>
      <xs:documentation>Dati reversale</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="tipo_operazione">
          <xs:annotation>
            <xs:documentation>Tipo di operazione</xs:documentation>
          </xs:annotation>
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:enumeration value="INSERIMENTO"/>
              <xs:enumeration value="VARIAZIONE"/>
              <xs:enumeration value="ANNULLO"/>
            </xs:restriction>
