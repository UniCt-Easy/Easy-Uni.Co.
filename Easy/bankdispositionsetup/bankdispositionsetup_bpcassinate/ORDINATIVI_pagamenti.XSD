    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="tipo_operazione">
          <xs:annotation>
            <xs:documentation>Tipo di operazione</xs:documentation>
          </xs:annotation>
          <xs:simpleType>
            <xs:restriction base="xs:string">
