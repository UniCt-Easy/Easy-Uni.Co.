                          <xsd:maxInclusive value="999999.99" />
                          <xsd:totalDigits value="8" />
                          <xsd:fractionDigits value="2" />
                        </xsd:restriction>
                      </xsd:simpleType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:choice>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:sequence>
