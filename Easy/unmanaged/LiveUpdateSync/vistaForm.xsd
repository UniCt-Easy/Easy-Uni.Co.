<?xml version="1.0" standalone="yes"?>
<xs:schema id="vistaForm" targetNamespace="http://tempuri.org/vista.xsd" xmlns:mstns="http://tempuri.org/vista.xsd" xmlns="http://tempuri.org/vista.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
  <xs:element name="vistaForm" msdata:IsDataSet="true">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element name="sync">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="id" type="xs:int" />
              <xs:element name="indirizzo" type="xs:string" minOccurs="0" />
              <xs:element name="port" type="xs:string" minOccurs="0" />
              <xs:element name="descrizione" type="xs:string" minOccurs="0" />
              <xs:element name="user" type="xs:string" minOccurs="0" />
              <xs:element name="pwd" type="xs:string" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="master">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="indirizzo" type="xs:string" minOccurs="0" />
              <xs:element name="user" type="xs:string" minOccurs="0" />
              <xs:element name="pwd" type="xs:string" minOccurs="0" />
              <xs:element name="flaglocale" type="xs:string" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
    <xs:unique name="vistaFormKey1" msdata:PrimaryKey="true">
      <xs:selector xpath=".//mstns:sync" />
      <xs:field xpath="mstns:id" />
    </xs:unique>
  </xs:element>
</xs:schema>