<?xml version="1.0" standalone="yes"?>
<xs:schema id="vistaForm" targetNamespace="http://tempuri.org/regionelistavista.xsd" xmlns:mstns="http://tempuri.org/regionelistavista.xsd" xmlns="http://tempuri.org/regionelistavista.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
  <xs:element name="vistaForm" msdata:IsDataSet="true">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element name="region">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="regioncode" type="xs:string" />
              <xs:element name="description" type="xs:string" />
              <xs:element name="cu" type="xs:string" />
              <xs:element name="ct" type="xs:dateTime" />
              <xs:element name="lu" type="xs:string" />
              <xs:element name="lt" type="xs:dateTime" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
    <xs:unique name="vistaFormKey1" msdata:PrimaryKey="true">
      <xs:selector xpath=".//mstns:region" />
      <xs:field xpath="mstns:regioncode" />
    </xs:unique>
  </xs:element>
</xs:schema>